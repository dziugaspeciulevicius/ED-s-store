{
  "name": "eds-store",
  "version": "2.0.0",
  "description": "ED's closet app",
  "main": "server.js",
  "type": "module",
  "dependencies": {
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "prettier": "^2.4.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.6"
  },
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "client-mui": "npm start --prefix frontend-mui",
    "client-mui-ts": "npm start --prefix frontend-mui-ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-mui": "concurrently \"npm run server\" \"npm run client-mui\"",
    "dev-mui-ts": "concurrently \"npm run server\" \"npm run client-mui-ts\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "prettier-run": "prettier --write './**/*.{js,jsx,ts,tsx,css,scss}' --config .prettierrc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dziugaspeciulevicius/ED-s-store.git"
  },
  "author": "Dziugas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dziugaspeciulevicius/ED-s-store/issues"
  },
  "homepage": "https://github.com/dziugaspeciulevicius/ED-s-store#readme"
}
